<AnchorPane id="AnchorPane" prefHeight="571.0" prefWidth="806.0" style="-fx-background-color: #424242;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.StadeController">
   <children>
      <ImageView fx:id="image_view" fitHeight="82.0" fitWidth="160.0" layoutX="74.0" layoutY="283.0" pickOnBounds="true" preserveRatio="true" />
      <VBox layoutX="14.0" layoutY="98.0" prefHeight="181.0" prefWidth="160.0" spacing="10.0">
         <children>
            <TextField fx:id="tfId" prefHeight="25.0" prefWidth="162.0" promptText="Numero de stade" />
            <TextField fx:id="tfNom" promptText="Nom du stade" />
            <TextField fx:id="tfAdresse" layoutX="10.0" layoutY="35.0" promptText="adresse" />
            <DatePicker fx:id="tfDateouverture" promptText="Date d'ouverture" />
            <TextField fx:id="tfCapacite" layoutX="10.0" layoutY="85.0" promptText="Capacite" />
         </children>
      </VBox>
      <TableView fx:id="tvStade" layoutX="232.0" layoutY="84.0" onMouseClicked="#handleMouseClicked" prefHeight="460.0" prefWidth="560.0" style="-fx-background-color: #212121;">
        <columns>
          <TableColumn fx:id="colId" prefWidth="23.0" text="ID" />
          <TableColumn fx:id="colNom" prefWidth="156.0" text="Nom" />
            <TableColumn fx:id="colAdresse" prefWidth="74.0" text="Adresse" />
            <TableColumn fx:id="colDateouverture" prefWidth="159.0" text="Dateouvertue" />
            <TableColumn fx:id="colCapacite" prefWidth="56.0" text="capacite" />
            <TableColumn fx:id="colImage" prefWidth="91.0" text="Image" />
        </columns>
      </TableView>
      <Button fx:id="btnInsert" layoutX="44.0" layoutY="409.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="123.0" style="-fx-background-color: #424242; -fx-background-radius: 40; -fx-border-color: #eeeeee;" text="Ajouter stade">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0" startX="0.5476190476190477">
               <stops>
                  <Stop color="#c29e0ed2" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill></Button>
      <Button fx:id="btnUpdate" layoutX="44.0" layoutY="459.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="123.0" style="-fx-background-color: #424242; -fx-border-color: #eeeeee;" text="Modifier stade">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0" startX="0.5476190476190477">
               <stops>
                  <Stop color="#c29e0ed2" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill></Button>
      <Button fx:id="btnDelete" layoutX="44.0" layoutY="511.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="123.0" style="-fx-background-color: #424242; -fx-border-color: #eeeeee;" text="Supprimer stade">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0" startX="0.5476190476190477">
               <stops>
                  <Stop color="#c29e0ed2" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill></Button>
      <TextField fx:id="filterField" layoutX="295.0" layoutY="40.0" prefHeight="25.0" prefWidth="74.0" promptText="Search .." style="-fx-background-radius: 12;" />
