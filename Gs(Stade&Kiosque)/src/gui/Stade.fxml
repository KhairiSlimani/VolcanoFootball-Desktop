         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label fx:id="file_path" layoutX="39.0" layoutY="369.0" prefHeight="17.0" prefWidth="115.0" />
      <Button layoutX="616.0" layoutY="36.0" mnemonicParsing="false" onAction="#Map" prefHeight="27.0" prefWidth="82.0" style="-fx-background-color: #424242; -fx-border-color: #eeeeee; -fx-background-radius: 15;" text="Localisation">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#bca810" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Button>
      <ImageView fitHeight="29.0" fitWidth="22.0" layoutX="698.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true">
