            <Image url="@../images/xmap-removebg-preview.png" />
         </image>
      </ImageView>
      <Button fx:id="imprimer" layoutX="509.0" layoutY="35.0" mnemonicParsing="false" onAction="#OnClickedPrint" prefHeight="29.0" prefWidth="82.0" style="-fx-background-color: #424242; -fx-background-border: 15; -fx-border-color: #eeeeee;" text="Imprimer">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#bca810" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Button>
      <Button layoutX="403.0" layoutY="35.0" mnemonicParsing="false" onAction="#stat" prefHeight="29.0" prefWidth="82.0" style="-fx-background-color: #424242; -fx-border-color: #eeeeee;" text="Statistiques">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#d6c03a" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill></Button>
   </children>
</AnchorPane>
